    "url": "https://github.com/131/remote-cp/issues"
  },
  "homepage": "https://github.com/131/remote-cp#readme",
  "dependencies": {
    "debug": "^2.3.2",
    "multiplex": "^6.7.0"
  },
  "devDependencies": {
    "eslint-plugin-ivs": "^1.0.7",
    "expect.js": "^0.3.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2"
  }
